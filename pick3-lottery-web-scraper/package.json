  ],
  "directories": {
    "lib": "src/main/javascript"
  },
  "main": "release/index.js",
  "devDependencies": {
    "graceful-fs": "^4.1.4",
    "grunt": "^1.0.3",
    "grunt-bump": "^0.8.0",
    "grunt-contrib-clean": "^0.7.0",
    "grunt-contrib-concat": "^1.0.0",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-jshint": "^1.1.0",
